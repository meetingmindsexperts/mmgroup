{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "mmgroup",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-27",
	"observability": {
		"enabled": true
	},
	/**
   * Environment Variables
   * VECTOR_STORE: "kv" (free) or "vectorize" (paid)
   * LLM_PROVIDER: "openai" or "workers-ai"
   * EMBEDDING_PROVIDER: "openai" or "workers-ai"
   */
	"vars": {
		"VECTOR_STORE": "kv",
		"LLM_PROVIDER": "openai",
		"EMBEDDING_PROVIDER": "openai",
		"ALLOWED_ORIGIN": "https://meetingmindsgroup.com"
	},
	/**
   * KV Namespace for vector storage (free tier)
   * Create with: wrangler kv namespace create VECTORS_KV
   */
	"kv_namespaces": [
		{
			"binding": "VECTORS_KV",
			"id": "5c5a1ea13b8f4df2ac575091fc09bdf3"
		}
	],
	/**
   * D1 Database for chat analytics
   */
	"d1_databases": [
		{
			"binding": "ANALYTICS_DB",
			"database_name": "mmgroup-analytics",
			"database_id": "e996d567-0f3c-4082-a655-d4a184600099"
		}
	]
	/**
   * Vectorize (uncomment when upgrading to paid plan)
   * Create with: wrangler vectorize create mmgroup-vectors --dimensions=1536 --metric=cosine
   */
	// "vectorize": [
	//   {
	//     "binding": "VECTORS_INDEX",
	//     "index_name": "mmgroup-vectors"
	//   }
	// ]
	/**
   * Workers AI (uncomment when using workers-ai provider)
   */
	// "ai": {
	//   "binding": "AI"
	// }
}